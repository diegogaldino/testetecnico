import java.util.Scanner;

public class Primos {

	public static void main(String[] args) {
		
        int intervalo = 15;

        for (int i = 2; i < intervalo; i++) {
            boolean ehPrimo = true;
            for (int j = 2; j <= i; j++) {
                if (((i % j) == 0) && (j != i)) {
                    ehPrimo = false;
                    break;
                }
            }

            if (ehPrimo) {
                System.out.println(i + " é Primo!");
            } else {
                System.out.println(i + " não é Primo!");
            }
        }
/*		int inicio, fim;
